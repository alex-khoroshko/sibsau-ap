<?xml version="1.0"?>

<PropertyList>
	<generic>
		<output>
			<line_separator>newline</line_separator>
			<var_separator>tab</var_separator>
 
 			<!--position-->
			<chunk>
				<node>/position/altitude-ft</node>
				<name>altitude</name>
				<type>float</type>
				<factor>0.3048</factor>
				<format>alt=%03.2f</format>
			</chunk>
			
			<chunk>
				<node>/position/ground-elev-m</node>
				<name>ground-elev</name>
				<type>float</type>
				<format>ground-elev=%03.2f</format>
			</chunk>
			
			<chunk>
				<node>/position/latitude-deg</node>
				<name>latitude</name>
				<type>float</type>
				<format>lat=%.10f</format>
			</chunk>
			 
			<chunk>
				<node>/position/longitude-deg</node>
				<name>longitude</name>
				<type>float</type>
				<format>lon=%.10f</format>
			</chunk>
 
 			<!--angles-->
			<chunk>
				<node>/orientation/roll-deg</node>
				<name>roll</name>
				<type>float</type>
				<factor>0.0174532</factor>
				<format>roll=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/orientation/pitch-deg</node>
				<name>pitch</name>
				<type>float</type>
				<factor>0.0174532</factor>
				<format>pitch=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/orientation/heading-deg</node>
				<name>heading</name>
				<type>float</type>
				<factor>0.0174532</factor>
				<format>yaw=%03.5f</format>
			</chunk>
			
			<!--rates-->
 			<chunk>
				<node>/orientation/roll-rate-degps</node>
				<name>roll_rate</name>
				<type>float</type>
				<factor>0.0174532</factor>
				<format>rate-roll=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/orientation/pitch-rate-degps</node>
				<name>pitch_rate</name>
				<type>float</type>
				<factor>0.0174532</factor>
				<format>rate-pitch=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/orientation/yaw-rate-degps</node>
				<name>yaw_rate</name>
				<type>float</type>
				<factor>0.0174532</factor>
				<format>rate-yaw=%03.5f</format>
			</chunk>
			
			<!--speeds-->
			<chunk>
				<node>/velocities/vertical-speed-fps</node>
				<name>vert-speed</name>
				<type>float</type>
				<factor>0.3048</factor>
				<format>vert-speed=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/velocities/speed-north-fps</node>
				<name>speed-north</name>
				<type>float</type>
				<factor>0.3048</factor>
				<format>speed-north=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/velocities//velocities/speed-east-fps</node>
				<name>speed-east</name>
				<type>float</type>
				<factor>0.3048</factor>
				<format>speed-east=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/velocities/airspeed-kt</node>
				<name>airspeed</name>
				<type>float</type>
				<factor>0.5144</factor>
				<format>airspeed=%03.5f</format>
			</chunk>
			
			<!--accelerations-->
			<chunk>
				<node>/accelerations/pilot/x-accel-fps_sec</node>
				<name>x-accel</name>
				<type>float</type>
				<factor>0.3048</factor>
				<format>x-accel=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/accelerations/pilot/y-accel-fps_sec</node>
				<name>y-accel</name>
				<type>float</type>
				<factor>0.3048</factor>
				<format>y-accel=%03.5f</format>
			</chunk>
			
			<chunk>
				<node>/accelerations/pilot/z-accel-fps_sec</node>
				<name>z-accel</name>
				<type>float</type>
				<factor>0.3048</factor>
				<format>z-accel=%03.5f</format>
			</chunk>
		</output>
	</generic>
</PropertyList>